@model IEnumerable<MVCLearnings.Models.Employee>
@{
    ViewBag.Title = "Employees";
}
<h2>@ViewBag.Title</h2>
<ul>
    @foreach (var employee in Model)
    {
        <li>@Html.ActionLink(@employee.EmployeeId.ToString() + "-" + @employee.Name, "Details", new { id = employee.EmployeeId })</li>
    }
</ul>
